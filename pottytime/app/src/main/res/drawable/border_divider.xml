<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item
        android:left="-1dp"
        android:bottom="-1dp"
        android:top="-1dp">
        <shape android:shape="rectangle" >
            <solid android:color="#ffdfe8e2" />

            <stroke
                android:width="1dp"
                android:color="#ff98b5a3"
                android:dashGap="2dp"
                android:dashWidth="2dp"/>
        </shape>
    </item>

</layer-list>